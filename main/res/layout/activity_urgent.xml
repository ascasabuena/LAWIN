<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/urgent"
    android:orientation="vertical"
    tools:context=".urgent">

    <ImageView
        android:id="@+id/home"
        android:layout_width="95dp"
        android:layout_height="74dp"
        android:layout_marginEnd="202dp"
        android:layout_marginBottom="118dp"
        app:layout_constraintBottom_toTopOf="@+id/check"
        app:layout_constraintEnd_toStartOf="@+id/reports"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <ImageView
        android:id="@+id/profile"
        android:layout_width="43dp"
        android:layout_height="42dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="6dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icon" />

    <ImageView
        android:id="@+id/reports"
        android:layout_width="65dp"
        android:layout_height="38dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toStartOf="@+id/profile"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/folder" />

    <TextView
        android:id="@+id/location_urgent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:textColor="@android:color/white"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="468dp" />

    <TextView
        android:id="@+id/near_location_urgent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:textColor="@android:color/white"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="497dp" />

    <TextView
        android:id="@+id/time_urgent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:textColor="@android:color/white"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="524dp" />

    <TextView
        android:id="@+id/distance_urgent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:textColor="@android:color/white"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="552dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CASE ID"
        android:textColor="@android:color/white"
        tools:layout_editor_absoluteX="131dp"
        tools:layout_editor_absoluteY="587dp" />

    <TextView
        android:id="@+id/case_id_urgent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:textColor="@android:color/white"
        tools:layout_editor_absoluteX="193dp"
        tools:layout_editor_absoluteY="587dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GENERATED"
        android:textColor="@android:color/white"
        tools:layout_editor_absoluteX="259dp"
        tools:layout_editor_absoluteY="587dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NEAREST STATION:"
        android:textColor="@android:color/white"
        tools:layout_editor_absoluteX="143dp"
        tools:layout_editor_absoluteY="649dp" />

    <TextView
        android:id="@+id/station_urgent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:textColor="@android:color/background_light"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="676dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="WEARABLE LOCATION"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="113dp" />

    <com.google.android.gms.maps.MapView
        android:id="@+id/map_urgent"
        android:layout_width="285dp"
        android:layout_height="243dp"
        tools:layout_editor_absoluteX="63dp"
        tools:layout_editor_absoluteY="192dp" />

</androidx.constraintlayout.widget.ConstraintLayout>